{
  "name": "sketches",
  "version": "0.0.1",
  "description": "a starting point for sketches",
  "main": "src/index.js",
  "scripts": {
    "dev": "node scripts/dev.js",
    "build": "node scripts/build.js",
    "start": "open http://localhost:8080 && cd docs && python -m SimpleHTTPServer 8080",
    "lint": "standard",
    "postinstall": "cp node_modules/standard/eslintrc.json .eslintrc || true",
    "create": "NAME=$(node scripts/gen_name.js) && touch src/$NAME.js && npm run start src/$NAME.js"
  },
  "author": "Taylor Baldwin",
  "license": "MIT",
  "dependencies": {
    "alea": "0.0.9",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-preset-es2015": "^6.9.0",
    "babelify": "^7.3.0",
    "budo": "^8.3.0",
    "dat-gui": "^0.5.0",
    "encode-object": "^0.2.2",
    "fontmetrics": "^1.0.0",
    "gl-vec2": "^1.0.0",
    "simplex-noise": "^2.2.0",
    "standard": "7.1.2",
    "tinycolor2": "^1.4.1",
    "underscore": "^1.8.3"
  },
  "devDependencies": {
    "browserify": "^13.1.0",
    "bundle-collapser": "^1.2.1",
    "mkdirp": "^0.5.1",
    "rimraf": "^2.5.4",
    "simple-html-index": "^1.4.0",
    "uglify-js": "^2.7.3"
  },
  "standard": {
    "ignore": [
      "docs"
    ]
  }
}
